{
  "name": "timeline-maker",
  "author": "Dana Harris",
  "version": "1.0.0",
  "description": "Timeline Maker is a Proof of Concept for building a Windows app using Angular 10, Electron, NodeJS and Sqlite3 as its base.",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "generate": "node ./server/generate.js > ./server/database.json",
    "server": "json-server --watch ./server/database.json",
    "dev": "concurrently \"npm run server\" \"npm run start\"",
    "electron": "ng build && electron .",
    "electron-aot": "ng build --prod && electron .",
    "electron:rebuild": "electron-rebuild -f -w sqlite3",
    "cypress:open": "cypress open",
    "cypress:clear": "cypress cache clear",
    "cypress:verify": "cypress verify",
    "cypress:run": "cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.6",
    "@angular/cdk": "^11.2.5",
    "@angular/common": "~11.2.6",
    "@angular/compiler": "~11.2.6",
    "@angular/core": "~11.2.6",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~11.2.6",
    "@angular/material": "^11.2.5",
    "@angular/platform-browser": "~11.2.6",
    "@angular/platform-browser-dynamic": "~11.2.6",
    "@angular/router": "~11.2.6",
    "@ngrx/effects": "^11.0.1",
    "@ngrx/entity": "^11.0.1",
    "@ngrx/schematics": "^11.0.1",
    "@ngrx/store": "^11.0.1",
    "@ngrx/store-devtools": "^11.0.1",
    "concurrently": "^5.3.0",
    "knex": "^0.21.19",
    "ngx-electron": "^2.2.0",
    "rxjs": "^6.6.6",
    "sqlite3": "^5.0.2",
    "tslib": "^2.1.0",
    "xvfb": "^0.4.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.5",
    "@angular/cli": "~11.2.5",
    "@angular/compiler-cli": "~11.2.6",
    "@types/jasmine": "^3.6.7",
    "@types/node": "^12.20.6",
    "codelyzer": "^6.0.0",
    "cypress": "^6.8.0",
    "electron": "^11.3.0",
    "electron-prebuilt-compile": "^8.2.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "^4.0.7"
  }
}
